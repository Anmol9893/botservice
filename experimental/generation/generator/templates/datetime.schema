{
    "$schema": "http://json-schema.org/draft-07/schema",
    "datetime": {
        "type": "object",
        "$entities": ["${concat('datetime:', property)}", "datetime"],
        "properties": {
            "type": {
                "type": "string"
            },
            "timex": {
                "type": "string"
            }
        },
        "required": ["type", "timex"],
        "$templates": ["datetimeEntity.lu","objectProperty.lg", "string_missing.dialog","datetimeEntity_add.dialog", "datetimeEntity_remove.dialog", "datetimeEntity_clear.dialog", "datetimeEntity_show.dialog"]
    }
}