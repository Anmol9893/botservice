{
    "$schema": "http://json-schema.org/draft-07/schema",
    "temperature": {
        "type": "object",
        "$entities": ["${concat('temperature:', property)}", "temperature"],
        "properties": {
            "number": {
                "type": "number"
            },
            "units": {
                "type": "string"
            }
        },
        "required": ["number", "units"],
        "$templates": ["temperatureEntity.lu","objectProperty.lg", "string_missing.dialog","temperatureEntity_add.dialog", "temperatureEntity_remove.dialog", "temperatureEntity_clear.dialog", "temperatureEntity_show.dialog"]
    }
}