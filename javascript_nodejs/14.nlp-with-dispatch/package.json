{
  "name": "nlp-with-dispatch",
  "version": "1.0.0",
  "description": "Bot Builder v4 example that demonstrates how dispatch should be used E2E.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "watch": "nodemon index.js",
    "installTools": "npm i -g ludown msbot luis-apis botdispatch qnamaker chatdown",
    "configure-services": "node deploymentScripts/configureServices.js",
    "dispatch-create": "dispatch create -b ./nlp-with-dispatch.bot | msbot connect dispatch --stdin",
    "dispatch-refresh": "dispatch refresh -b ./nlp-with-dispatch.bot",
    "configure-and-start": "npm run configure-services & npm run dispatch-create & node index.js"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^4.0.0-preview1.1089",
    "botframework-config": "^4.0.0-preview1.1089",
    "botbuilder-ai": "^4.0.0-preview1.1089",
    "botbuilder-dialogs": "^4.0.0-preview1.1089",
    "botbuilder-azure": "^4.0.0-preview1.1089",
    "dotenv": "^6.0.0",
    "restify": "^6.3.4"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
